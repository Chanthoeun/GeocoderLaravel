{
    "name": "toin0u/geocoder-laravel",
    "description": "Geocoder Service Provider for Laravel",
    "keywords": [
        "laravel",
        "geocoder",
        "geocoding"
    ],
    "homepage": "http://geocoder-php.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Corcy",
            "email": "contact@sbin.dk",
            "homepage": "http://sbin.dk",
            "role": "Developer"
        },
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com",
            "homepage": "https://genealabs.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/cache": "~5.0",
        "illuminate/support": "~5.0",
        "willdurand/geocoder": "^4.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "~1.4",
        "laravel/laravel": "5.4.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "php-http/message": "^1.5",
        "php-http/curl-client": "^1.7",
        "php-http/discovery": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "geocoder-php/bing-maps-provider": "^4.0",
        "geocoder-php/chain-provider": "^4.0",
        "geocoder-php/common-http": "^4.0",
        "geocoder-php/geo-plugin-provider": "^4.0@dev",
        "geocoder-php/google-maps-provider": "^4.0",
        "geocoder-php/maxmind-binary-provider": "^4.0",
        "satooshi/php-coveralls" : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Geocoder\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/laravel/laravel/app/",
            "Geocoder\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0-dev"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
